{
  "id": "a6BDIv2HRupc",
  "name": "app-next-auth",
  "description": "Next.js UI with Auth",
  "custom": {
    "root": "scripts"
  },
  "secrets": {
    "base": "base"
  },
  "docker": {
    "default": {
      "root": "docker",
      "image": "gcr.io/ghostmind-core/templates-ui",
      "env_based": false,
      "context_dir": "app"
    }
  },
  "compose": {
    "default": {
      "root": "local"
    }
  },
  "terraform": {
    "core": {
      "path": "infra",
      "global": false,
      "containers": ["default"]
    }
  },
  "tunnel": {
    "default": {
      "hostname": "${APP}.ghostmind.app",
      "service": "http://localhost:5001"
    }
  },
  "routines": {
    "dev": "run custom dev build up",
    "activate": "run action local ui -W --no-reuse"
  },
  "tags": ["app", "next-auth"]
}
