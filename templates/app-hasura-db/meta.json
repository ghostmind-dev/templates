{
  "id": "lWatCfm3C45R",
  "name": "app-hasura-db",
  "type": "template",
  "terraform": {
    "core": {
      "path": "run",
      "global": false,
      "containers": ["default"]
    }
  },
  "docker": {
    "default": {
      "root": "docker",
      "image": "gcr.io/ghostmind-core/templates-state",
      "env_based": false
    }
  },
  "compose": {
    "default": {
      "root": "local",
      "filename": "compose.yaml"
    }
  },
  "routines": {
    "dev": "run custom dev build up",
    "console": "run custom dev console",
    "activate": "run action local state -W --no-reuse"
  },
  "tags": ["app", "hasura", "db"]
}
