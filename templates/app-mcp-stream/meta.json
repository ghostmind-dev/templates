{
  "id": "8Ye2KLHFyNF1",
  "name": "mcp-stream",
  "type": "template",
  "secrets": {
    "base": "base"
  },
  "docker": {
    "default": {
      "root": "docker",
      "image": "gcr.io/ghostmind-core/templates-mcp-stream",
      "env_based": false,
      "context_dir": "app"
    }
  },
  "compose": {
    "default": {
      "root": "local",
      "filename": "compose.yaml"
    }
  },
  "terraform": {
    "default": {
      "path": "infra",
      "global": false,
      "containers": ["default"]
    }
  },
  "mcp": {
    "from_mcp_stream": {
      "url": "${SERVER_URL}",
      "headers": {
        "Authorization": "${SERVER_TOKEN}"
      }
    }
  }
}
