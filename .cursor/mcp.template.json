{
    "mcpServers": {
        "hasura-db": {
            "command": "/workspaces/templates/dev/run/bin/cmd.ts",
            "args": [
                "misc",
                "mcp",
                "npx",
                "mcp-graphql",
                "--env",
                "/workspaces/templates/templates/app-hasura-db/.env.mcp"
            ],
            "env": {}
        },
        "get-temperature": {
            "url": "http://localhost:3000/mcp",
            "headers": {
                "Authorization": "APP-MCP-STREAM-SERVER-TOKEN"
            }
        }
    }
}